{
  "name": "trevenant",
  "version": "1.0.0",
  "description": "Pokemon Damage Calculator built with Elm",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html --dist-dir dist",
    "build:prod": "parcel build src/index.html --dist-dir dist --public-url .",
    "typecheck": "elm make src/Main.elm --output=/dev/null",
    "test": "elm-test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@pkmn/dex": "^0.10.2",
    "@pkmn/img": "^0.3.1",
    "@smogon/calc": "^0.10.0",
    "elm-test": "^0.19.1-revision17"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "autoprefixer": "^10.4.22",
    "daisyui": "^5.5.5",
    "parcel": "^2.12.0",
    "parcel-transformer-elm": "^0.0.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17"
  }
}
